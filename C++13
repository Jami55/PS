#include <iostream>
using namespace std;

int main () {
        string s = "Dawatus Sunnah Conferance";
        cout << s.length() << "\n";
        cout << s[3] << "\n";
        s[7] = 't';
        cout << s << "\n";
        cout << s.find("Dawatus") << "\n";
        cout << s.substr(3, 2) << "\n";
        return 0;
}
