#include <iostream>
#include <cstring>
#include <cmath>
using namespace std;

int main () {
        bool is_male = true, is_tall = true;
        if (is_male && is_tall) {
                cout << "You are male and tall" << "\n";
        }
        else if (is_male && !is_tall) {
                cout << "You are male but not tall" << "\n";
        } else if (!is_male && is_tall) {
                cout << "You are not male but tall" << "\n";
        } else if (!is_male && !is_tall) {
                cout << "You are not male and not tall" << "\n";
        }
        return 0;
}
