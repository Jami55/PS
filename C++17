#include <iostream>
#include <cmath>
using namespace std;

int main()
{
        int x, y;
        cout << "Enter first number : ";
        cin >> x;
        cout << "Enter second number : ";
        cin >> y;
        cout << "Sum : " << x + y << "\n";
        cout << "Difference : " << x - y << "\n";
        cout << "Quotient : " << x / y << "\n";
        cout << "Product : " << x * y << "\n";
        return 0;
}
