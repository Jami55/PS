#include <iostream>
#include <cstring>
#include <cmath>
using namespace std ;

int f (int count) {
            int i = 0 ;
            while (i < count) {
                        cout << "Hello " ;
                        i ++ ;
            }
            cout << "\n" ;
            cout << "That's all." << "\n" ;
            return 1 ;
}

int main () {
            int count ;
            cout << "How many greetings do you want ? " ;
            cin >> count ;
            f (count) ;
            return 0 ;
}
