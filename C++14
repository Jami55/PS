#include <iostream>
#include <cmath>
using namespace std;

int main () {
        int x = 18;
        x++;
        cout << x << "\n";
        cout << x + 1 << "\n";
        cout << x - 1 << "\n";
        cout << x / 2 << "\n";
        cout << x % 3 << "\n";
        cout << pow(x, 2) << "\n";
        cout << round(x) << "\n";
        cout << sqrt(x) << "\n";
        return 0;
}
